<p id="notice"><%= notice %></p>

<h1>Listing Transactions</h1>

<table>
  <thead>
    <tr>
      <th>Creditcardnumber</th>
      <th>Cardholdername</th>
      <th>Type</th>
      <th>Securitypass</th>
      <th>Expirationdate</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transactions.each do |transaction| %>
      <tr>
        <td><%= transaction.creditCardNumber %></td>
        <td><%= transaction.cardholderName %></td>
        <td><%= transaction.type %></td>
        <td><%= transaction.securityPass %></td>
        <td><%= transaction.expirationDate %></td>
        <td><%= transaction.price %></td>
        <td><%= link_to 'Show', transaction %></td>
        <td><%= link_to 'Edit', edit_transaction_path(transaction) %></td>
        <td><%= link_to 'Destroy', transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transaction', new_transaction_path %>
